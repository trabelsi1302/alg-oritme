ALGORITHM Checkpoint4
VAR
    caractere := INTEGER
    mot := INTEGER
    voyelle :=INTEGER
    phrase := STRING
    c:=CHAR
    i,j:=INTEGER
BEGIN
InsertionSort(A):
  for i = 1 to length(A) do
    key = A[i]                // key is the current element being inserted
    j = i - 1                 // j is the index of the element to the left of key in the sorted subarray
    
    // Shift elements to the right to make room for key
    while j >= 0 and A[j] > key do
      A[j+1] = A[j]
      j = j - 1

END
